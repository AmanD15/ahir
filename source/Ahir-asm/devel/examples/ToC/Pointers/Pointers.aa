
$storage u: $uint<32>
$storage v: $uint<32>


$module [increment]
 $in (incr_a: $uint<32> )
 $out (incr_b: $uint<32>) 
$is
{
         $storage incr_temp: $uint<32>
	 incr_t1 := ($cast ($pointer< $uint<32> >) incr_a)
         incr_temp  := ->(incr_t1)
         incr_b  := (incr_temp + 1)
}

$module [passpointer]
    $in (a:$uint<32>)
    $out (b:$uint<32>)
$is
{
	u := a
	pu := @(u)
	q  := ($cast ($uint<32>) pu)
	$call increment (q) (b)
}

